{
  "manifest_version": 3,
  "name": "Custom Favicon Changer",
  "version": "1.0",
  "description": "Changes favicon to green when URL contains user-defined keyword",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
} 